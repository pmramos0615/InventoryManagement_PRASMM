@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <meta name="description" content="POS - Bootstrap Admin Template">
    <meta name="keywords" content="admin, estimates, bootstrap, business, corporate, creative, management, minimal, modern,  html5, responsive">
    <meta name="author" content="Dreamguys - Bootstrap Admin Template">
    <meta name="robots" content="noindex, nofollow">
    <title>PRASMM Inventory Management System</title>

    <!-- Favicon -->

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link href="https://cdn.jsdelivr.net/npm/themify-icons/css/themify-icons.css" rel="stylesheet">
    <script src="https://unpkg.com/feather-icons"></script>
    <!-- Select2 CSS -->
    <link rel="stylesheet" href="~/plugins/select2/css/select2.min.css">

    <!-- animation CSS -->
    <link rel="stylesheet" href="~/css/animate.css">

    <!-- Datatable CSS -->
    <link rel="stylesheet" href="~/css/dataTables.bootstrap4.min.css">

    <!-- Fontawesome CSS -->
    <link rel="stylesheet" href="~/plugins/fontawesome/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/plugins/fontawesome/css/all.min.css">

    

    <!-- Main CSS -->
    
    <link rel="stylesheet" href="~/css/style.css">

</head>
<body>
    @* <div id="global-loader"> 
        <div class="whirly-loader"> </div>
    </div> 
    Uncomment this if mas bet yung may page loader
    *@
    <!-- Main Wrapper -->
    <div class="main-wrapper">

        <!-- Header -->
        @{
            Html.RenderPartial("_Header");
        }
        <!-- Header -->
        <!-- Sidebar -->
        @{
            Html.RenderPartial("_SideBar");
        }
        <!-- /Sidebar -->

        <div class="page-wrapper">
            @RenderBody()
        </div>

    </div>
    <!-- /Main Wrapper -->
    <!-- jQuery -->

	<script src="~/js/jquery-3.6.0.min.js"></script>

    <!-- Feather Icon JS -->
    <script src="~/js/feather.min.js"></script>

    <!-- Slimscroll JS -->
    <script src="~/js/jquery.slimscroll.min.js"></script>

    <!-- Select2 JS -->
    <script src="~/plugins/select2/js/select2.min.js"></script>
   @*  <script src="~/plugins/select2/js/custom-select.js"></script> *@

    

    <!-- Datatable JS -->
    <script src="~/js/jquery.dataTables.min.js"></script>
    <script src="~/js/dataTables.bootstrap4.min.js"></script>

    <!-- Bootstrap Core JS -->
    <script src="~/js/bootstrap.bundle.min.js"></script>

    <!-- Chart JS -->
    <script src="~/plugins/apexchart/apexcharts.min.js"></script>
    <script src="~/plugins/apexchart/chart-data.js"></script>

    <!-- Sweetalert 2 -->
    @* I DID UPDATE THIS INTO THE CURRENT VERSION THE VERSION .11 is ALREADY DEPRECATED *@
    <script src="~/plugins/sweetalert/sweetalert2.all.min.js"></script>
    <script src="~/plugins/sweetalert/sweetalerts.min.js"></script>

    <!-- Custom JS -->
    <script src="~/js/script.js"></script>
    @RenderSection("scripts", required: false)
    <!-- Mainly scripts -->


</body>
</html>